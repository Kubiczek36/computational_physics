# Makefile for 2dising
#
# 4-May-2020 (CL)
#
#**********************************************************************#

F90=gfortran
F90FLAGS=-O3

all: 2dising

2dising: 2dising.f90
	${F90} ${F90FLAGS} -o 2dising 2dising.f90

clean:
	rm -fv 2dising

#**********************************************************************#
